{
  "name": "@ton/ton",
  "version": "15.0.0",
  "repository": "https://github.com/ton-org/ton.git",
  "author": "Whales Corp. <developers@whalescorp.com>",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "docs": "rm -fr docs && typedoc src/index.ts",
    "build": "rm -fr dist && tsc --declaration",
    "tc": "tsc --noEmit",
    "test": "jest --verbose --runInBand",
    "release": "yarn build && yarn release-it --npm.yarn1"
  },
  "devDependencies": {
    "@release-it/keep-a-changelog": "5.0.0",
    "@ton/core": "0.57.0",
    "@ton/crypto": "3.3.0",
    "@ton/emulator": "2.1.3",
    "@types/jest": "29.5.12",
    "@types/node": "22.5.4",
    "buffer": "6.0.3",
    "expect": "29.7.0",
    "jest": "29.7.0",
    "jest-mock": "29.7.0",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-jasmine": "5.1.0",
    "karma-typescript": "5.5.4",
    "karma-webpack": "5.0.1",
    "prando": "6.0.1",
    "release-it": "17.6.0",
    "ts-jest": "29.2.5",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "typedoc": "0.26.7",
    "typescript": "5.6.2",
    "webpack": "5.94.0"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "dataloader": "^2.2.2",
    "symbol.inspect": "1.0.3",
    "teslabot": "^1.5.0",
    "zod": "^3.23.8"
  },
  "peerDependencies": {
    "@ton/core": ">=0.57.0",
    "@ton/crypto": ">=3.3.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "release-it": {
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/keep-a-changelog": {
        "filename": "CHANGELOG.md"
      }
    }
  },
  "packageManager": "pnpm@9.10.0"
}
